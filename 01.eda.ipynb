# Exploratory Data Analysis for Startup Dataset

import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

# Load dataset
data = pd.read_csv("startup_data.csv")

# Basic overview
print(data.head())
print(data.info())

# Startup count by sector
plt.figure()
data['Sector'].value_counts().plot(kind='bar')
plt.title("Sector-wise Distribution of Startups")
plt.xlabel("Sector")
plt.ylabel("Number of Startups")
plt.show()

# Startup count by city
plt.figure()
data['City'].value_counts().plot(kind='bar')
plt.title("City-wise Startup Distribution")
plt.xlabel("City")
plt.ylabel("Number of Startups")
plt.show()

# Operational status distribution
plt.figure()
data['Status'].value_counts().plot(kind='pie', autopct='%1.1f%%')
plt.title("Operational Status of Startups")
plt.ylabel("")
plt.show()
